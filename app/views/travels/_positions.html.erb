<div class="row form-inline">
  <div class="col-xs-12"><h3>Positions</h3></div>
</div>
<table class="table" id="positions-table">
  <thead>
    <th>Date & Time</th>
    <th>Latitude</th>
    <th>Longitude</th>
  </thead>
  <% travel.positions.each do |position| %>
    <tr>
      <td><%= Time.at(position.timestamp) %></td>
      <td><%= position.latitude %></td>
      <td><%= position.longitude %></td>
    </tr>
  <% end %>
</table>
